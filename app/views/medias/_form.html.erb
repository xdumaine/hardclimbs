<%= simple_form_for @media, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :url %>
  <%= f.input :description %>
  <%= f.input :media_types_id %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                medias_path, :class => 'btn' %>
  </div>
<% end %>
